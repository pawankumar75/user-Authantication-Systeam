<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd" id="WebApp_ID" version="6.0">
  <display-name>Signup</display-name>
  <welcome-file-list>
   
    <welcome-file>index.jsp</welcome-file>
 
  </welcome-file-list>
  <servlet>
  <servlet-name>regdemo</servlet-name>
  <servlet-class>com.pawan.reg.Registration</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>regdemo</servlet-name>
  <url-pattern>/reg</url-pattern>
  </servlet-mapping>
 
 
  <servlet>
  <servlet-name>logindemo</servlet-name>
  <servlet-class>com.pawan.reg.Login</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>logindemo</servlet-name>
  <url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>forgotdemo</servlet-name>
  <servlet-class>com.pawan.reg.ForgotPassword</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>forgotdemo</servlet-name>
  <url-pattern>/forgotPassword</url-pattern>
  </servlet-mapping>
  
   <servlet>
  <servlet-name>validatedemo</servlet-name>
  <servlet-class>com.pawan.reg.ValidateOtp</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>validatedemo</servlet-name>
  <url-pattern>/ValidateOtp</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>newpassworddemo</servlet-name>
  <servlet-class>com.pawan.reg.NewPassword</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>newpassworddemo</servlet-name>
  <url-pattern>/newPassword</url-pattern>
  </servlet-mapping>
  
</web-app>